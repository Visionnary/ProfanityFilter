{
    "manifest_versions": 2,
    "name": "Webreader",
    "description": "Read text from website",
    "version": "1.0",
    "background":{
        "service_worker": "main.js"
    },
    "icons":{
      "16": "/icons/image-16.png",
      "48": "/icons/image-48.png",
      "128": "/icons/image-128.png"
    },
    "page_action":{
      "default_icon":{
        "16": "/icons/image-16.png",
        "48": "/icons/image-48.png",
        "128": "/icons/image-128.png",
      "default_popup": "menu.html"
      }
    },
    "permissions": ["activeTab"]

}